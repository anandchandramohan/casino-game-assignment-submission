{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "npm: start-react",
      "type": "shell",
      "command": "powershell",
      "args": [
        "-NoExit",
        "-Command",
        "Set-ExecutionPolicy -Scope Process -ExecutionPolicy Bypass; npm start"
      ],
      "options": {
        "cwd": "${workspaceFolder}/client"
      },
      "isBackground": true,
      "problemMatcher": "$tsc-watch"
    }
  ]
}
